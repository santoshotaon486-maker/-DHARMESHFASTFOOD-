<canvas id="cv" width="320" height="300"></canvas>

<script>
const cv=document.getElementById("cv");
const cx=cv.getContext("2d");
let ball={x:160,y:50,r:15,vy:2};
let gravity=0.4;

function loop(){
    cx.clearRect(0,0,320,300);
    ball.vy+=gravity;
    ball.y+=ball.vy;

    if(ball.y+ball.r>300){
        ball.y=300-ball.r;
        ball.vy=-ball.vy*0.8;
    }

    cx.beginPath();
    cx.fillStyle="brown";
    cx.arc(ball.x,ball.y,ball.r,0,Math.PI*2);
    cx.fill();

    requestAnimationFrame(loop);
}
loop();
</script>

